{
  "extends": "tslint:latest",
  "rules": {
    "array-type": [
        true,
        "array"
    ],
    "no-console": [
        true
    ],
    "curly": false,
    "one-line": [
        true,
        "check-catch",
        "check-finally",
        "check-whitespace"
      ],
    "interface-name": [
        false
    ],
    "max-line-length": [
        false
    ],
    "max-classes-per-file": [
        false
    ]
  },
  "jsRules": {
  },
  "rulesDirectory": [
  ]
}